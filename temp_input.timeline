event E1 {
    title = "Founding of City";
    date = 500;
    importance = high;
};

event E2 {
    title = "First Council";
    date = 480;
    importance = medium;
};

period P1 {
    title = "Early Growth";
    start = 500;
    end = 650;
    importance = low;
};

timeline cityHistory {
    title = "City History";
    E1, E2, P1;
};

main {
    for item in cityHistory {
        if (item.importance == HIGH) {
            modify item {
                importance = low;
            };
        } else {
            modify item {
                importance = medium;
            };
        };
    };
    export cityHistory;
};
